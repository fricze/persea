  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "dev": "npm-run-all -p watch-css start-js storybook-watch",
    "build-js": "node ./node_modules/react-scripts/bin/react-scripts.js build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "flow": "flow",
    "storybook-watch": "start-storybook -p 9010 -s public",
    "storybook": "start-storybook -p 9010 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.15",
    "@storybook/addon-links": "^3.2.15",
    "@storybook/react": "^3.2.15",
    "json-loader": "^0.5.7",
    "raw-loader": "^0.5.1"
